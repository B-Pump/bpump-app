{
    "expo": {
        "name": "Robot B-Pump",
        "description": "Application of the B-Pump robot, your 100% Robotic sports coach!",
        "slug": "bpump-app",
        "owner": "wiizz",
        "version": "1.0.0",
        "githubUrl": "https://github.com/wiizzl/bpump-app",
        "scheme": "acme",
        "plugins": ["expo-router"],
        "icon": "assets/images/icon.png",
        "platforms": ["android", "web"],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "4a752c12-2f60-477e-93fd-efe6db60af67"
            }
        },
        "splash": {
            "image": "assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "android": {
            "package": "com.wiizz.bpumpapp",
            "adaptiveIcon": {
                "foregroundImage": "assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "favicon": "assets/images/favicon.png"
        }
    }
}
