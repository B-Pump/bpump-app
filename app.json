{
    "expo": {
        "name": "Robot B-Pump",
        "description": "Application of the B-Pump robot, your 100% Robotic sports coach !",
        "slug": "bpump",
        "version": "2.0.0",
        "owner": "wiizz",
        "githubUrl": "https://github.com/B-Pump/bpump-app",
        "scheme": "bpump",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "platforms": ["android", "ios"],
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "plugins": ["expo-router", "expo-font", "expo-secure-store"]
    }
}
