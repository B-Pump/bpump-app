{
    "expo": {
        "name": "Robot B-Pump",
        "description": "Application of the B-Pump robot, your 100% Robotic sports coach !",
        "slug": "bpump-app",
        "version": "2.0.0",
        "owner": "wiizz",
        "githubUrl": "https://github.com/B-Pump/bpump-app",
        "scheme": "bpump",
        "orientation": "default",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "platforms": ["android", "ios"],
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.wiizz.bpumpapp"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.wiizz.bpumpapp",
            "versionCode": 3
        },
        "plugins": [
            "expo-router",
            "expo-font",
            "expo-secure-store",
            [
                "expo-build-properties",
                {
                    "android": {
                        "usesCleartextTraffic": true
                    }
                }
            ]
        ],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "c21561c8-d4c5-4922-8490-7ded40513502"
            }
        }
    }
}
